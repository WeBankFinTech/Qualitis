{
  "name": "dqm-ui-next",
  "version": "1.12.0",
  "description": "新版qualities页面",
  "scripts": {
    "build": "fes build",
    "prod": "FES_ENV=prod fes build",
    "analyze": "ANALYZE=1 fes build",
    "dev": "fes dev",
    "test-sit": "FES_ENV=sit fes build",
    "test-dev": "FES_ENV=dev fes build",
    "test-uat": "FES_ENV=uat fes build",
    "test:unit": "fes test:unit",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "keywords": [
    "qualities",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "files": [
    ".eslintrc.js",
    ".gitignore",
    ".fes.js",
    ".fes.prod.js",
    "mock.js",
    "package.json",
    "README.md",
    "tsconfig.json",
    "/src",
    "/config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeBankFinTech/fes.js.git",
    "directory": "packages/fes-template"
  },
  "author": "harrywan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WeBankFinTech/fes.js/issues"
  },
  "homepage": "https://github.com/WeBankFinTech/fes.js#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "15.0.0",
    "@fesjs/builder-webpack": "^3.0.14",
    "@vue/compiler-sfc": "3.2.26",
    "@webank/eslint-config-webank": "0.4.2",
    "babel-plugin-import": "^1.13.8",
    "commitlint": "15.0.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2"
  },
  "dependencies": {
    "@fesjs/fes": "3.1.12",
    "@fesjs/fes-design": "0.8.68",
    "@fesjs/plugin-access": "3.1.3",
    "@fesjs/plugin-enums": "3.0.0",
    "@fesjs/plugin-icon": "3.0.0",
    "@fesjs/plugin-locale": "4.3.0",
    "@fesjs/plugin-model": "3.0.1",
    "@fesjs/plugin-monaco-editor": "3.0.3",
    "@fesjs/plugin-request": "3.0.8",
    "@fesjs/plugin-watermark": "3.0.0",
    "@fesjs/traction-widget": "1.11.0",
    "@micro-zoe/micro-app": "1.0.0-beta.3",
    "@webank/fes-plugin-pmbank-um": "3.1.1",
    "babel-plugin-syntax-jsx": "6.18.0",
    "clipboard": "^2.0.11",
    "core-js": "3.33.0",
    "date-fns": "2.28.0",
    "dayjs": "1.11.5",
    "echarts": "5.2.2",
    "lodash-es": "4.17.21",
    "mitt": "3.0.0",
    "sql-formatter": "15.3.2",
    "vue": "3.2.47",
    "vuex": "4.0.2"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue,fes,wxa}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E $HUSKY_GIT_PARAMS"
    }
  }
}
